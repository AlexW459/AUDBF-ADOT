FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "initialConditions";

dimensions      [0 0 -1 0 0 0 0];

internalField uniform $specificTurbulenceDissipationRate;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform $specificTurbulenceDissipationRate;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform $specificTurbulenceDissipationRate;
        value           uniform $specificTurbulenceDissipationRate;
    }
    upper
    {
        type inletOutlet;
        inletValue  uniform $specificTurbulenceDissipationRate;
        value       uniform $specificTurbulenceDissipationRate;
    }
    lower
    {
        type inletOutlet;
        inletValue  uniform $specificTurbulenceDissipationRate;
        value       uniform $specificTurbulenceDissipationRate;
    }
    front
    {
        type inletOutlet;
        inletValue  uniform $specificTurbulenceDissipationRate;
        value       uniform $specificTurbulenceDissipationRate;
    }
    back
    {
        type inletOutlet;
        inletValue  uniform $specificTurbulenceDissipationRate;
        value       uniform $specificTurbulenceDissipationRate;
    }
    aircraftModel
    {
        type omegaWallFunction;
        value uniform $specificTurbulenceDissipationRate;
    }
    horizontalStabiliser
    {
        type omegaWallFunction;
        value uniform $specificTurbulenceDissipationRate;
    }
}
