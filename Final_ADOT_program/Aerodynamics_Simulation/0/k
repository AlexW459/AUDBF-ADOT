FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.0216;

boundaryField
{
    front
    {
        type            inletOutlet;
        inletValue      uniform 0.0216;
        value           uniform 0.0216;
    }
    back
    {
        type            inletOutlet;
        inletValue      uniform 0.0216;
        value           uniform 0.0216;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.0216;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.0216;
        value           uniform 0.0216;
    }
    lower
    {
        type            inletOutlet;
        inletValue      uniform 0.0216;
        value           uniform 0.0216;
    }
    upper
    {
        type            inletOutlet;
        inletValue      uniform 0.0216;
        value           uniform 0.0216;
    }
    aircraftModel
    {
        type            kqRWallFunction;
        value           uniform 0.0216;
    }
    horizontalStabiliser
    {
        type            kqRWallFunction;
        value           uniform 0.0216;
    }
}


