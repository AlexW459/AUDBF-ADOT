FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.24;

boundaryField
{
    front
    {
        type            inletOutlet;
        inletValue      uniform 1e-06;
        value           uniform 1e-06;
    }
    back
    {
        type            inletOutlet;
        inletValue      uniform 1e-06;
        value           uniform 1e-06;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 0.24;
    }
    outlet
    {
        type            zeroGradient;
    }
    lower
    {
        type            inletOutlet;
        inletValue      uniform 1e-06;
        value           uniform 1e-06;
    }
    upper
    {
        type            inletOutlet;
        inletValue      uniform 1e-06;
        value           uniform 1e-06;
    }
    aircraftModel
    {
        type            kqRWallFunction;
        value           uniform 0.24;
    }
}


