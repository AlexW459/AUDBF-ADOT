FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


actions
(

    //Thin box to measure velocity in front of tail
    {
        name   tailUpstreamBoxSet;
        type   cellSet;
        action new;
        source boxToCell;
        //TailUpstreamBox
        box (-.897315 -0.525497 -0.031964) (-.859583 0.525497 0.011218);
    }

);


